{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "lib": [
      "es6",
      "dom"
    ],
    "typeRoots": [
      "./node_modules/@types"
    ],
    "esModuleInterop": true,
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
  },
  "rootDir": "src",
}